			"\t\t\t#ExternalModuleDependencies\t=\t@(",
			"\t\t\t#\t@{",
			"\t\t\t#\t\tModuleName\t\t=\t'ActiveDirectory'",
			"\t\t\t#\t\tModuleVersion\t=\t'1.0.1.0'",
			"\t\t\t#\t\tGUID\t\t\t=\t'43c15630-959c-49e4-a977-758c5cc93408'",
			"\t\t\t#\t}",
